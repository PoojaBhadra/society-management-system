<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/unnamed"
    tools:context="com.example.tejashree.societymanagementsystem.gui.memberGUI.memberCircleMenus.MemberProfileActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="Name"
        android:textSize="20dp"
        android:layout_marginStart="26dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/nameFieldProfile"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="26dp" />

    <TextView
        android:id="@+id/nameFieldProfile"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:textSize="20dp"
        android:text=""
        android:layout_marginStart="26dp"
        android:layout_marginEnd="28dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="23dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toRightOf="@+id/textView9"
        android:layout_marginLeft="26dp"
        android:layout_marginRight="28dp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:textSize="20dp"
        android:text="Society"
        android:layout_marginEnd="14dp"
        app:layout_constraintRight_toLeftOf="@+id/societyNameProfileField"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintBaseline_toBaselineOf="@+id/societyNameProfileField"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="14dp" />

    <TextView
        android:id="@+id/societyNameProfileField"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:text=""
        android:textSize="20dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/nameFieldProfile"
        android:layout_marginTop="34dp"
        app:layout_constraintTop_toBottomOf="@+id/nameFieldProfile"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/nameFieldProfile" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Admin"
        android:textSize="20dp"
        android:layout_marginStart="26dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/adminNameProfile"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="26dp" />

    <TextView
        android:id="@+id/adminNameProfile"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:text=""
        android:textSize="20dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/societyNameProfileField"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@+id/societyNameProfileField"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/societyNameProfileField" />



    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/ele"
        android:layout_marginStart="36dp"
        tools:layout_constraintTop_creator="1"
        android:layout_marginTop="6dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/nameFieldProfile"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="36dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:textSize="20dp"
        android:text="No Of Complaints"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="350dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/noOfComplaintsFieldProfile"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp" />

    <TextView
        android:id="@+id/noOfComplaintsFieldProfile"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:textSize="20dp"
        android:text=""
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="350dp"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="31dp"
        android:layout_marginEnd="28dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="31dp"
        app:layout_constraintTop_toBottomOf="@+id/adminNameProfile"
        app:layout_constraintLeft_toRightOf="@+id/textView21"
        android:layout_marginLeft="31dp"
        android:layout_marginRight="28dp" />
</android.support.constraint.ConstraintLayout>
