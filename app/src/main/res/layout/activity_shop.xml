<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"

    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/unnamed"
    tools:context=".helpActivities.PestControlActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/pcCallButton"
        android:layout_width="68dp"
        android:layout_height="70dp"
        android:background="@drawable/call"
        android:layout_marginEnd="21dp"
        app:layout_constraintRight_toLeftOf="@+id/pcMsgButton"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        android:layout_marginRight="21dp" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="347dp"
        android:layout_height="117dp"
        android:layout_marginBottom="82dp"
        android:layout_marginEnd="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="73dp"
        android:text="Name: Nikhil Bhelhekar
Address: Shop No. B2/23, Bavdhan, Pune-21"
        android:textColor="#000000"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/pcCallButton1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="16dp"
        android:text="Name: Gaurav Bagade
Address: Shop No. A1/20, Bavdhan, Pune-21"
        android:textColor="#000000"
        android:textSize="30dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="15dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintBottom_toTopOf="@+id/pcCallButton1"
        app:layout_constraintTop_toBottomOf="@+id/pcMsgButton" />

    <Button
        android:id="@+id/pcCallButton1"
        android:layout_width="68dp"
        android:layout_height="70dp"
        android:background="@drawable/call"
        app:layout_constraintRight_toLeftOf="@+id/pcMsgButton1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="16dp"
        android:layout_marginRight="27dp" />

    <Button
        android:id="@+id/pcMsgButton"
        android:layout_width="88dp"
        android:layout_height="0dp"
        android:background="@drawable/message"
        android:layout_marginEnd="1dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/pcCallButton"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="1dp"
        app:layout_constraintTop_toBottomOf="@+id/textView12"
        android:layout_marginRight="1dp"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/pcMsgButton1"
        android:layout_width="88dp"
        android:layout_height="71dp"
        android:background="@drawable/message"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="2dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBaseline_toBaselineOf="@+id/pcCallButton1"
        tools:layout_constraintBaseline_creator="1"
        android:layout_marginRight="2dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="258dp"
        android:layout_height="58dp"
        android:layout_marginLeft="16dp"
        android:text="Shop Market"
        android:textAppearance="@style/Base.TextAppearance.AppCompat.Body1"
        android:textSize="45sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintTop_creator="1" />

</android.support.constraint.ConstraintLayout>